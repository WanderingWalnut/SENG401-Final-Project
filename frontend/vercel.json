{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/login", "destination": "/login", "statusCode": 200 },
    { "source": "/signup", "destination": "/signup", "statusCode": 200 },
    { "source": "/chat", "destination": "/chat", "statusCode": 200 },
    { "source": "/upload", "destination": "/upload", "statusCode": 200 },
    { "source": "/transactions", "destination": "/transactions", "statusCode": 200 }
  ]
}
