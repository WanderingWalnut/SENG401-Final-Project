{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "rewrites": [
      { "source": "/", "destination": "/landingPage" },
      { "source": "/login", "destination": "/LoginPage" },
      { "source": "/signup", "destination": "/SignUpPage" },
      { "source": "/chat", "destination": "/chatPage" },
      { "source": "/transactions", "destination": "/TransactionPage" },
      { "source": "/upload", "destination": "/UploadPage" }
    ]
  }